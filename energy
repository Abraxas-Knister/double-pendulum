set term png
set xlabel "Time, a.u."
set ylabel "Error in Energy, Powers of 10"
set log y
set format y '%L'
set rmargin at screen .75
set label at screen .77,.8 left "The time unit\n is 600 ms,\n this is \n equal to\n sqrt(l/g)"
stats 'pendata' u 1:6 nooutput
set out 'energy.png'
pl 'pendata' u 1:(abs($6-STATS_mean_y)) w l t ''
